{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "buildOptions": {
        "debugType": "portable",
        "copyToOutput": {
            "include": [
                "xunit.runner.json"
            ]
        }
    },
    "dependencies": {
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "xunit": "2.2.0-*",
        "dotnet-test-xunit": "2.2.0-*",
        "Microsoft.AspNetCore.TestHost": "1.0.0",
        "System.Net.Http": "4.1.0",
        "Xunit.Extensions.TestOrdering": {
            "target": "project"
        },
        "Dict.Srv": {
            "target": "project"
        }
    },
    "frameworks": {
        "netcoreapp1.0": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.1"
                }
            },
            "imports": [
                "dotnet5.4",
                "portable-net451+win8"
            ]
        }
    }
}